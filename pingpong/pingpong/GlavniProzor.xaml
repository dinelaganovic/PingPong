<Window x:Class="pingpong.GlavniProzor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:pingpong"
        mc:Ignorable="d"
        Title="GlavniProzor" Height="200" Width="200">
    <StackPanel>
        <TextBlock Text="Ime i prezime prvog igraca" HorizontalAlignment="Center"/>
        <TextBox x:Name="txtIP1"/>
        <TextBlock Text="Ime i prezime drugog igraca" HorizontalAlignment="Center"/>
        <TextBox x:Name="txtIP2"/>
        <TextBlock Text="Pocetna brzina kretanja loptice" HorizontalAlignment="Center"/>
        <TextBox x:Name="txtBrzina"/>
        <Button Content="Pocni igru" Margin="10" Click="Button_Click"/>
    </StackPanel>
</Window>
